<h1>Create new User</h1>

<%= form_for @user do |form| %>   <!-- form for generates new form -->
	Name: <%= form.text_field :name, placeholder: "Name" %>
	Age: <%= form.text_field :age, placeholder: "Age" %>
	Password: <%= form.text_field :password, placeholder: "Password" %>
	<%= form.submit %>
<% end %>

